<body style="display: flex; justify-content: center;">
    <div>
        <h1>Contents To Be edited Are Listed Below</h1>
        <%data.forEach((val,index)=>{%>
            <div>
              <form action="/editblog" method="post">
                  <h1 style="font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;"><%=(index+1)%>.<%=val.title%></h1>
                  <input type="hidden" name="yellow" value="<%=val._id%>">
                  <input type="hidden" name="editer" value="<%=val.title%>">
                  <input type="hidden" name="count" value="<%=(index+1)%>">
                  <input type="submit" value="edit"
                   style="width: 100px; height: 40px; border-radius:25px; border-style: none;
             color: white;
             background-color:#8F00FF;
              text-align: center;
              text-decoration: none;
              font-size: 16px;
              margin: 4px 2px;
              cursor: pointer;
              -webkit-transition-duration: 0.4s;
              transition-duration: 0.4s;">
              </form>
             
            </div>
        <%})%>
    </div>
    
</body>
